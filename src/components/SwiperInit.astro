<script>
    import Swiper from 'swiper';
    import 'swiper/css';
  if (typeof window !== 'undefined') {
    document.addEventListener('DOMContentLoaded', () => {
      const swiper = new Swiper('.swiper-container', {
        direction: 'vertical',
        slidesPerView: 3,
        spaceBetween: 30,
        pagination: {
          el: '.swiper-pagination',
          clickable: true,
        },
      });
    });
  }
</script>